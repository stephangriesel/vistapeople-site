---
export interface Props {
  post: any;
}

const { post } = Astro.props;
console.log("hero component",post.frontmatter)
---
<section
  id="home"
  class="relative overflow-hidden bg-primary pt-[120px] md:pt-[130px] lg:pt-[160px]"
>
  <div class="container">
    <div class="-mx-4 flex flex-wrap items-center">
      <div class="w-full px-4">
        <div
          class="hero-content wow fadeInUp mx-auto max-w-[780px] text-center"
          data-wow-delay=".2s"
        >
          <h1
            class="mb-6 text-3xl font-bold leading-snug text-white sm:text-4xl sm:leading-snug lg:text-5xl lg:leading-[1.2]"
          >
            {post.frontmatter.title}
            </h1>
          <p
            class="mx-auto mb-9 max-w-[600px] font-medium text-base text-white sm:text-lg sm:leading-[1.44]"
          >
          {post.frontmatter.description}
          </p>
          <ul class="mb-10 flex gap-5 flex-wrap items-center justify-center">
            <li>
              <a
                href={post.frontmatter.ctaleftlink}
                class="inline-flex items-center justify-center rounded-md bg-white py-[14px] px-7 text-center text-base font-medium text-dark shadow-1 transition duration-300 ease-in-out hover:text-body-color hover:bg-gray-2"
              >
                {post.frontmatter.ctaleft}
              </a>
            </li>
            <li>
              <a
                href={post.frontmatter.ctarightlink}
                target="_blank"
                class="flex items-center gap-4 py-[14px] rounded-md bg-white/[0.12] px-6 text-base font-medium text-white transition duration-300 ease-in-out hover:bg-white hover:text-dark"
              >
              <!-- <svg xmlns="http://www.w3.org/2000/svg" class="text-white text-black hover:fill-current hover:text-black duration-300 ease-in-out" fill="#3659f9" width="32" height="32" viewBox="0 0 256 256"><path d="M229.66,58.34l-32-32a8,8,0,0,0-11.32,0l-96,96A8,8,0,0,0,88,128v32a8,8,0,0,0,8,8h32a8,8,0,0,0,5.66-2.34l96-96A8,8,0,0,0,229.66,58.34ZM124.69,152H104V131.31l64-64L188.69,88ZM200,76.69,179.31,56,192,43.31,212.69,64ZM224,128v80a16,16,0,0,1-16,16H48a16,16,0,0,1-16-16V48A16,16,0,0,1,48,32h80a8,8,0,0,1,0,16H48V208H208V128a8,8,0,0,1,16,0Z"></path></svg> -->
                {post.frontmatter.ctaright}
              </a>
            </li>
          </ul>
        </div>
